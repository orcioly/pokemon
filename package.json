{
  "name": "pokemon",
  "version": "1.0.0",
  "description": "API para buscar Pokemons",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=local ts-node-dev --inspect --transpile-only --ignore-watch node_modules ./src/main/server.ts",
    "build": "tsc",
    "postbuild": "copyfiles public/* public/**/* dist",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/main/server.js"
